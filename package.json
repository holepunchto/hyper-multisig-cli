{
  "name": "hyper-multisig-cli",
  "version": "0.0.0",
  "description": "CLI of hyper-multisig",
  "main": "bin.js",
  "bin": {
    "hyper-multisig": "./bin.js",
    "hyper-multisig-bare": "./bin-bare.js"
  },
  "files": [
    "bin-bare.js",
    "bin.js"
  ],
  "scripts": {
    "format": "prettier --write . && lunte --fix",
    "lint": "prettier --check . && lunte",
    "test": "brittle -c test.js",
    "test-bare": "brittle-bare -c test.js"
  },
  "dependencies": {
    "bare-fs": "^4.5.4",
    "corestore": "^7.8.0",
    "graceful-goodbye": "^1.3.3",
    "hyper-multisig": "^0.0.0",
    "hypercore-id-encoding": "^1.3.0",
    "hypercore-signing-request": "^4.0.2",
    "hyperdrive": "^13.2.1",
    "hyperswarm": "^4.16.0",
    "paparam": "^1.10.0",
    "z32": "^1.1.0"
  },
  "devDependencies": {
    "b4a": "^1.7.4",
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.2",
    "bare-subprocess": "^5.2.2",
    "brittle": "^3.19.1",
    "compact-encoding": "^2.18.0",
    "hypercore-crypto": "^3.6.1",
    "hypercore-sign": "^3.0.0",
    "hyperdht": "^6.29.0",
    "lunte": "^1.6.0",
    "newline-decoder": "^1.0.2",
    "prettier-config-holepunch": "^2.0.0",
    "resolve-reject-promise": "^1.1.0",
    "sodium-native": "^5.0.10",
    "which-runtime": "^1.3.2"
  },
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    }
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/hyper-multisig-cli.git"
  },
  "bugs": {
    "url": "https://github.com/holepunchto/hyper-multisig-cli/issues"
  },
  "homepage": "https://github.com/holepunchto/hyper-multisig-cli#readme"
}
